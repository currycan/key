{
    "inbounds": [
        {
            "port": "${XRAY_KCP_PORT}",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "${XRAY_KCP_UUID}",
                        "email": "vless-kcp@gmail.com"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "kcp",
                "kcpSettings": {
                    "uplinkCapacity": 100,
                    "downlinkCapacity": 100,
                    "congestion": true,
                    "readBufferSize": 5,
                    "writeBufferSize": 5,
                    "seed": "${XRAY_KCP_SEED}"
                }
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ]
            }
        }
    ]
}
