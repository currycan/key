{
    "inbounds": [
        {
            "listen": "/dev/shm/udsxhttp.sock,0666",
            "protocol": "vless",
            "tag": "XHTTP_IN",
            "settings": {
                "clients": [
                    {
                        "id": "${XRAY_XHTTP_UUID}",
                        "level": 0,
                        "email": "vless@xhttp.reality"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "xhttp",
                "xhttpSettings": {
                    "host": "",
                    "path": "/${XRAY_XHTTP_URL_PATH}",
                    "mode": "auto",
                    "extra": {
                        "noSSEHeader": false,
                        "scMaxEachPostBytes": 1000000,
                        "scMaxBufferedPosts": 30,
                        "xPaddingBytes": "100-1000"
                    }
                },
                "sockopt": {
                    "tcpFastOpen": true,
                    "acceptProxyProtocol": false,
                    "tcpcongestion": "bbr",
                    "tcpMptcp": true,
                    "tcpNoDelay": true
                }
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ],
                "metadataOnly": false,
                "routeOnly": true
            }
        }
    ]
}
