{
    "inbounds": [
        {
            "listen": "/dev/shm/udsvmess.sock,0666",
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "${XRAY_VMESS_UUID}",
                        "email": "xray-vmess@gmail.com"
                    }
                ]
            },
            "streamSettings": {
                "network": "httpupgrade",
                "httpupgradeSettings": {
                    "path": "/${XRAY_VMESS_URL_PATH}"
                }
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ]
            }
        }
    ]
}
