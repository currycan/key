//  "CDN": "www.csgo.com"
{
    "inbounds": [
        {
            "type": "vless",
            "tag": "sing-box vless-ws-tls",
            "listen": "::",
            "listen_port": ${PORT_VLESS_WS},
            "tcp_fast_open": false,
            "proxy_protocol": false,
            "users": [
                {
                    "name": "sing-box",
                    "uuid": "${SB_UUID}"
                }
            ],
            "transport": {
                "type": "ws",
                "path": "/${SB_UUID}-vless",
                "max_early_data": 2048,
                "early_data_header_name": "Sec-WebSocket-Protocol"
            },
            "multiplex": {
                "enabled": true,
                "padding": true,
                "brutal": {
                    "enabled": ${IS_BRUTAL},
                    "up_mbps": 2500,
                    "down_mbps": 2500
                }
            }
        }
    ]
}
