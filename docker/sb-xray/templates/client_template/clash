- {name: "${REGION_INFO}|${NODE_NAME}|xtls-reality", server: ${DOMAIN}, port: ${PORT_XTLS_REALITY}, type: vless, tls: true, packet-encoding: xudp, uuid: ${SB_UUID}, servername: addons.mozilla.org, skip-cert-verify: false, udp: true, tfo: false}
